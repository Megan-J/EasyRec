<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ViewRecommendationController">
   <children>
      <AnchorPane prefHeight="500.0" prefWidth="500.0">
         <children>
            <TextArea fx:id="textBox" layoutX="50.0" layoutY="50.0" prefHeight="400.0" prefWidth="400.0" />
            <Label layoutX="97.0" layoutY="5.0" text="View Recommendation">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <Button fx:id="returnToEdit" layoutX="32.0" layoutY="461.0" mnemonicParsing="false" onAction="#openEditMenu" text="Edit" />
            <Button fx:id="saveRec" layoutX="430.0" layoutY="461.0" mnemonicParsing="false" onAction="#saveRec" text="Save" />
         </children>
      </AnchorPane>
   </children>
</VBox>
